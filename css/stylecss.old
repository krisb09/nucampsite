body {
    font-family: "Open Sans", sans-serif;
}

h1, h2, h3 {
    font-family: "Lobster", cursive;
}

h2 {
    color: blue;
}

header h2 {
    color: #cec8ff
}

.site-footer {
    background: linear-gradient(#cec8ff, white);
    padding: 20px 0;
}
.jumbotron {
    margin: 0;
    padding: 30px;
    background-image: linear-gradient(to right, #5637DD, #3046c5, #60106b);
    color: #fff;  
}

.row-content {
    padding: 50px 0;
    border-bottom: 1px ridge;
    min-height: 200px;
}

.navbar {
    background-color: #901CAD;
}



a {
    color:black;
    text-decoration: none;
}

a>.reserveText:hover {
    color:rgb(255, 255, 255);
    text-decoration: none;
}

button.btn:hover {
    background-color: rgba(214, 4, 251, 0.621);
}

.tab-content {
    border: 1px solid #DDD;
    border-top: none;
    padding: 10px;
}

a.title.collapsed {
    text-decoration: none;
}

.carousel-caption {
    background-color: rgba(0, 0, 0, 0.5);
}

.carousel {
    border: solid #5637DD;
}

.card-cont {
    
    margin-left: 159px;
}

#carouselButton {
    position: absolute;
    right: 0;
    bottom: 0;
    z-index: 100;
}
